<%= render partial: 'employer_dashboard' %>


<div class="container">
 	<div class="row">
		<div class="container">
			<div class="row">
				<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { multipart: true, method: :put }) do |f| %>
                <%= devise_error_messages! %>

                <div class="company-edit--section company-edit--profile" id="company-edit-1">
                    <div class="field">
                        <label class="form-labels">Employer Email</label>
                        <%= f.text_field :email, autofocus: true, class: "form-control", required: true %>
                    </div>
 

                <% end %>


                <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { multipart: true, method: :put }) do |f| %>
                <%= devise_error_messages! %>

                    <div class="actions">
                        <%= f.submit "Update", class: "btn btn-primary" %>
                    </div>

                <% end %>

                </div>
			</div>
		</div>
	</div>
</div>