<%= render partial: 'employer_dashboard' %>

<div class="container">
 	<div class="row">
		<% @listings.each do |listing| %>
			<%= link_to listing_path(listing) do %>
				<div class="container">
					<div class="row">
						<h1><%= listing.job_title %></h1>
					</div>
				</div>
			<% end %>
		<% end %>
	</div>
</div>